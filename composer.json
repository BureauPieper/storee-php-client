    "autoload": {
        "psr-4": { "Bureaupieper\\StoreeClient\\": "src" }
    },
    "autoload-dev": {
      "psr-4": { "Bureaupieper\\StoreeClient\\": "tests" }
    },
    "require": {
        "php": "~5.5",
